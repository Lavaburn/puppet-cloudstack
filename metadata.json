{
  "name": "Lavaburn-cloudstack",
  "version": "1.0.0",
  "summary": "Puppet module for managing Apache Cloudstack",
  "author": "Nicolas Truyens",
  "license": "Apache-2.0",
  "project_page": "https://github.com/Lavaburn/puppet-cloudstack",
  "source": "https://github.com/Lavaburn/puppet-cloudstack",
  "issues_url": "https://github.com/Lavaburn/puppet-cloudstack/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6.x",
        "7.x"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=4.6.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">=2.0.0"
    },
    {
      "name": "puppetlabs/mysql",
      "version_requirement": ">=2.3.0"
    },
    {
      "name": "puppetlabs/wget",
      "version_requirement": ">=1.5.0"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">=1.1.0"
    }    
  ],
  "tags": [
    "cloudstack",
    "cloud"
  ]
}
